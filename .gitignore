# Ignore directories generated by Composer
#/vendor/
#/core/
#/modules/contrib/
#/themes/contrib/
#/profiles/contrib/
#/libraries/

# Ignore configuration files that may contain sensitive information
/sites/*/*settings*.local.php
/sites/*/services*.yml

# Ignore paths that may contain user-generated content
/sites/*/files
/sites/*/public
/sites/*/private
/sites/*/files-public
/sites/*/files-private

# Ignore paths that may contain temporary files
/sites/*/translations
/sites/*/tmp
/sites/*/cache

# Ignore IDE files
/.idea/
/.vscode/
*.iml
*.code-workspace
.DS_store

# Ignore all log files
*.log

# Ignore node modules
node_modules

# Ignore some css related stuff
*.css.map
*.css.map.map
.sass-cache

# Ignore database dumps
*.sql
*.sql.gz

# Ignore uwdrupal symlink
/uwdrupal
